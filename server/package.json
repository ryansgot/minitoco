{
  "name": "minitoco",
  "version": "0.0.1",
  "description": "A small-scale simulation of a Tocos trading system.",
  "author": "ryan",
  "main": "src/index.ts",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build:dev": "rimraf ./build && tsc --build src",
    "build": "rm -rf ./build && tsc --build src",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "start": "npm run build && npx ts-node ./src/index.ts",
    "start:prod": "npx ts-node ./src/index.ts",
    "start:dev": "npx nodemon -L",
    "test:unit": "jest"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.13",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "babel-jest": "^29.5.0",
    "eslint": "^8.37.0",
    "install": "^0.13.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "nodemon": "^3.0.1",
    "npm": "^9.8.1",
    "prisma": "^4.14.1",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
